{
    "name": "codelytv/php-bootstrap",
    "description": "Starting point if you want to bootstrap a project in PHP following best practices.",
    "type": "project",
    "keywords": [
        "bootstrap",
        "skeleton",
        "kata",
        "TDD",
        "boilerplate"
    ],
    "homepage": "https://codely.tv",
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Ferrer",
            "email": "javier.ferrer@codely.tv",
            "homepage": "https://codely.tv",
            "role": "Developer"
        },
        {
            "name": "Rafa GÃ³mez",
            "email": "rafa.gomez@codely.tv",
            "homepage": "https://codely.tv",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/yaml": "^4.3",
        "ramsey/uuid": "^3.9",
        "symfony/messenger": "^4.3",
        "symfony/validator": "^4.3",
        "lambdish/phunctional": "2.0",
        "ext-json": "*",
        "dg/bypass-finals": "^1.1",
        "symfony/flex": "^1.6",
        "furqansiddiqui/erc20-php": "^0.2.1",
        "fakerphp/faker": "^1.13"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.4",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^7.2",
        "symfony/var-dumper": "^4.4",
        "php-parallel-lint/php-parallel-lint": "^1.1",
        "behat/behat": "^3.5",
        "behat/mink-extension": "^2.3",
        "behat/mink-browserkit-driver": "^1.3",
        "friends-of-behat/symfony-extension": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Challenge\\App\\": "apps/Challenge/src",
            "Challenge\\": "src/Challenge",
            "Ethereum\\": "src/Ethereum",
            "Shared\\": "src/Shared"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
